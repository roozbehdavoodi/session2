package com.company;

import java.util.Scanner;

public class Main {

    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        System.out.println("Enter the value of a:");
        int j;
        int i;
        int x;
        int y;
        int a = input.nextInt();
        System.out.println("Enter the value of b:");
        int b = input.nextInt();
        System.out.println("Enter the coefficient e:");
        int e = input.nextInt();
        System.out.println("Enter the coefficient f:");
        int f = input.nextInt();
        System.out.println("Enter the value of c:");
        int c = input.nextInt();
        System.out.println("Enter the value of d:");
        int d = input.nextInt();
        i=b*c;
        j=a*d;
        if (i !=j ) {
            System.out.println("The equation is insoluble");
        }
        else {
            x = ((e*d)-(b*f))/((a*d)-(b*c));
            y = ((a*f)-(e*c))/((a*d)-(b*c));
            System.out.println(x);
            System.out.println(y);
        }
    }
}
